#include <iostream>
#include <math.h>
using namespace std;
int main()
{
	int n, s1 = 0, s2 = 0,t1,t2;
	cout << "enter the number " << endl;
	cin >> n;
	s1 = ((5 * n * n) + 4);
	s2 = ((5 * n * n) - 4);
	t1=sqrt(s1);
	t2=sqrt(s2);
	if(s1==t1*t1||s2==t2*t2)
	{
		cout<<"its a fibanacci number "<<endl;
	}
	else 
	{cout<<"its not an fibanocci number "<<endl;}
	return 0;	
}
